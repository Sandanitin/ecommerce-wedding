{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://ecommerce-wedding-backend.onrender.com/api/$1"
    },
    {
      "source": "/uploads/(.*)",
      "destination": "https://ecommerce-wedding-backend.onrender.com/uploads/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
